word = input()
upper = 0
lower = 0
for i in word:
    if i.isupper():
        upper += 1
    elif i.islower():
        lower += 1
        
if upper > lower:
     newword =word.upper()
elif upper < lower:
     newword =word.lower()
elif upper == lower:
     newword =word.lower()
print(newword)
    
    
   
